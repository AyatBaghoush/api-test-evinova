{
  "name": "api-test-evinova",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "cucumber-js",
    "test:ci": "npx cucumber-js --format json:./reports/cucumber.json",
    "report:generate": "npx allure generate --clean ./reports/allure-results -o ./reports/allure-report",
    "report:open": "npx allure open ./reports/allure-report",
    "test:report": "npm run test || true && npm run report:generate && npm run report:open",
    "test:coverage": "nyc --silent npm run test &&  nyc report --reporter=lcov --reporter=html --reporter=text-summary && open ./coverage/index.html",
    "load-report" : "open ./load-report/k6-report.html"
  },
  "repository": {
    "type": "gsit",
    "url": "git+https://github.com/AyatBaghoush/api-test-evinova.git"
  },
  "keywords": [],
  "author": "",
